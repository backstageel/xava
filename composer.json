{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "url": "https://github.com/hostmoz/blade-bootstrap-components.git",
            "type": "git"
        },
        {
            "url": "https://github.com/backstageel/laravel-money.git",
            "type": "git"
        }
    ],
    "require": {
        "php": "^8.1",
        "akaunting/laravel-money": "dev-master",
        "barryvdh/laravel-dompdf": "^2.0",
        "bensampo/laravel-enum": "^6.3",
        "dompdf/dompdf": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "guzzlehttp/guzzle": "^7.2",
        "hostmoz/blade-bootstrap-components": "dev-main",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/telescope": "^4.14",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^2.12",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-permission": "^5.9",
        "yajra/laravel-datatables": "^10.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "deployer/deployer": "^7.1",
        "kitloong/laravel-migrations-generator": "^6.8",
        "laravel/breeze": "^1.19",
        "laravel/dusk": "^7.7",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.4",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.2",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\States\\": "database/States/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
